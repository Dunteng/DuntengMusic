/* components/progress-bar/cmp.wxss */
.container{
  display: flex;
  align-items: center;
}

.control {
  position: relative;
  flex: 1;
}
.movable-area{
  width: 100%;
  height: 34rpx;
  position: absolute;
  bottom: -14rpx;
  left: 0;
}
.movable-view{
  width: 36rpx;
  height: 36rpx;
  background-color: #fefefe;
  border-radius: 50%;
}
.time{
  color: #fff;
}