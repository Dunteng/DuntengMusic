/* miniprogram/pages/blog/blog.wxss */
.password-layer {
  min-height: 100%;
  width: 100%;
  z-index: 1000001;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tip {
  margin-top: 20rpx;
  font-size: 48rpx;
  color: #666;
}
.password-input {
  margin-top: 20rpx;
  text-align: center;
  font-size: 40rpx;
  height: 80rpx;
  border: 1rpx solid #666;
  border-radius: 50rpx;
  background-color: #f5f5f5;
}
.header{
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  background-color: #fff;
  height: 100rpx;
  position: fixed; /* 想要搜索框固定定位 */
  top: 0;
  left: 0;
  width: 100%;
  z-index: 100000;
  box-sizing: border-box;
}
.header .icon-fabu1{
  font-size: 60rpx;
  color: #d43c33;
}
.publish-container{
  width: 68rpx;
  height: 68rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.search-container{
  flex: 1;
  z-index: 999999;
}

.blog-list{
  margin-top: 120rpx;
}

.blog-panel {
  margin-bottom: 20rpx;
  box-sizing: border-box;
}



